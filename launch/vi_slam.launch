<!-- DATASET -->
<launch>
    <!-- VI-SLAM main thread-->
    <node pkg="vi_slam" type="vi_slam" name="main"  
        args="-filePath=/home/lujano/Documents/V1_02_medium/mav0/state_groundtruth_estimate0/data.csv 
              -imuPath=/home/lujano/Documents/V1_02_medium/mav0/imu0/data.csv 
              -imagePath=/home/lujano/Documents/V1_02_medium/mav0/cam0/data/"
        clear_params="true" 
        output="screen"/>
        
    <!-- Rviz visualize-->
    <node pkg="rviz" type="rviz" name="rviz"/>

    <!-- rqt_plot visualizer-->
    <node name="rqt_gui" pkg="rqt_gui" type="rqt_gui"/>
        
    <node name="ImuFilter" pkg="imu_filter_madgwick" type="imu_filter_node"
         args="_use_mag:=false _use_magnectic_field_msg:=false 
         _gain:=0.1 _world_frame:=nwu _stateless:=false 
         _publish_debug_topics:=true _constant_dt:=0.005"
    />
</launch>

